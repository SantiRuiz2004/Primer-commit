#include <stdio.h>
#include <stdlib.h>



int main(void)
{

	setbuf(stdout,NULL);

	int edad;

	printf("Ingrese su edad: ");
	scanf("%d", &edad);

	printf("su edad es: %d", edad);


	return EXIT_SUCCESS;
}

